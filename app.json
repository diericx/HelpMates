{
  "expo": {
    "name": "HelpMates",
    "description": "Get better grades while making money in college.",
    "slug": "helpmates",
    "privacy": "public",
    "sdkVersion": "26.0.0",
    "platforms": ["ios"],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#18c0ea"
    },
    "notification": {
      "icon": "./assets/icon-notification.png",
      "color": "#18c0ea"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    }
  },
  "infoPlist": {
    "NSPhotoLibraryUsageDescription":
      "This app uses the camera roll to allow users to select a profile picture."
  }
}
